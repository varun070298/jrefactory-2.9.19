/*
 * Program structuring syntax follows.
 */

ASTCompilationUnit CompilationUnit() :
{
    Token tok;
}
{
  [ PackageDeclaration() ]
  ( ImportDeclaration() )*
  ( TypeDeclaration() )*
  tok=<EOF>
  {
    jjtThis.addSpecial("EOF", tok.specialToken);
    return jjtThis;
  }
}

void PackageDeclaration() :
{
    Token tok;
}
{
  tok="package" {jjtThis.addSpecial("package", tok.specialToken);}
  Name()
  tok=";" {jjtThis.addSpecial("semicolon", tok.specialToken);}
}

void ImportDeclaration() :
{
    Token tok = null;
    Token specialPoint = null;
    int count = 0;
}
{
  specialPoint="import" {jjtThis.addSpecial("import", specialPoint.specialToken);}
  [ specialPoint="static"
    {
      jjtThis.addSpecial("static", specialPoint.specialToken);
      jjtThis.setStaticImport(true);
    }
  ]
  Name()
    [
      specialPoint="." {jjtThis.addSpecial("period", specialPoint.specialToken);}
      tok="*" {jjtThis.addSpecial("star", tok.specialToken);}
    ]
  (
    specialPoint=";" 
    {
       count++;
       if (count==1) {
          jjtThis.addSpecial("semicolon", specialPoint.specialToken);
          jjtThis.setImportPackage(tok != null);
       }
    }
  )*
}

void TypeDeclaration() :
{
}
{
  LOOKAHEAD( ("abstract" | "public" | "strictfp" | ("@" Annotation()) )* "@" "interface" )
  AnnotationTypeDeclaration()
|
  LOOKAHEAD( ( "abstract" | "final" | "public" | "strictfp" | ("@" Annotation()) )* "class" )
  ClassDeclaration()
|
  LOOKAHEAD( ("public" | "protected" | "private" | "final" | ("@" Annotation()) )* "enum" )
  EnumDeclaration()
|
  InterfaceDeclaration()
}


/*
 * Declaration syntax follows.
 */

void ClassDeclaration() :
{
  Token tok;
  int count=0;
}
{
  (
    (tok="abstract" {jjtThis.setAbstract(); jjtThis.addSpecial("abstract", tok.specialToken);} ) |
    (tok="final"    {jjtThis.setFinal(); jjtThis.addSpecial("final", tok.specialToken);} ) |
    (tok="strictfp" {jjtThis.setStrict(); jjtThis.addSpecial("strictfp", tok.specialToken);} ) |
    (tok="public"   {jjtThis.setPublic(); jjtThis.addSpecial("public", tok.specialToken);} ) |
    (tok="@" Annotation() {jjtThis.addSpecial("@."+count, tok.specialToken); count++;} )
  )*
  UnmodifiedClassDeclaration()
  [ ";" ]
}

void UnmodifiedClassDeclaration() :
{
    Token tok;
    Token specialToken;
}
{
  specialToken="class" {jjtThis.addSpecial("class", specialToken.specialToken);}
  (
    tok=<IDENTIFIER>
    {
      jjtThis.setName(tok.image);
      jjtThis.addSpecial("id", tok.specialToken);
    }
  )
  [
    TypeParameters()
  ]
  [
    specialToken="extends"  {jjtThis.addSpecial("extends", specialToken.specialToken);}
    ClassOrInterfaceType()
  ]
  [
    specialToken="implements"  {jjtThis.addSpecial("implements", specialToken.specialToken);}
    GenericNameList()
  ]
  ClassBody()
}


void Annotation() :
{
  Token specialToken;
  Token tok;
}
{
  //specialToken="@" {jjtThis.addSpecial("@", specialToken.specialToken);}
  Identifier()
  [ specialToken="(" {jjtThis.setMarkerAnnotation(false); jjtThis.addSpecial("begin", specialToken.specialToken);}
    [ LOOKAHEAD(2) MemberValuePairs() | MemberValue() ]
    specialToken=")" {jjtThis.addSpecial("end", specialToken.specialToken);}
  ]
}


void MemberValuePairs() :
{
  Token specialToken;
  int count=0;
}
{
  MemberValuePair()
  ( specialToken="," {jjtThis.addSpecial("comma."+count++, specialToken.specialToken);}
     MemberValuePair()
  )*
}


void MemberValuePair() :
{
  Token specialToken;
}
{
  Identifier()
  specialToken="=" {jjtThis.addSpecial("=", specialToken.specialToken);}
  MemberValue()
}


void MemberValue() :
{
  Token tok;
}
{
  tok="@" Annotation()  {jjtThis.addSpecial("@", tok.specialToken); }
|
  MemberValueArrayInitializer()
|
  ConditionalExpression()
}


void MemberValueArrayInitializer() :
{
  Token specialToken;
  int count=0;
}
{
  specialToken="{" {jjtThis.addSpecial("{", specialToken.specialToken);}
  [ MemberValue()
    ( LOOKAHEAD(2)
      "," {jjtThis.addSpecial("comma."+count++, specialToken.specialToken);}
      MemberValue()
    )*
    [ "," ]
  ]
  specialToken="}" {jjtThis.addSpecial("}", specialToken.specialToken);}
}


void TypeParameters() :
{
  Token specialToken;
}
{
  specialToken="<"  {
    if (!jdk1_5) {
      throw new ParseException("Type Parameters are only available in JDK 1.5 or later generics" );
    }

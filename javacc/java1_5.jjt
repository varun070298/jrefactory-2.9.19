/*
 * A lookahead of 2 is required below since "Name" can be followed
 * by a ".*" when used in the context of an "ImportDeclaration".
 */
{
    Token tok;
    Token period;
    int count = 0;
}
{
  tok=<IDENTIFIER> {jjtThis.addSpecial("id" + count, tok.specialToken);}
  {
    if (TESTING) {
        jjtThis.testingOnly__setBeginLine( tok.beginLine);
        jjtThis.testingOnly__setBeginColumn( tok.beginColumn);
    }
    jjtThis.addNamePart(tok.image);
    count++;
  }
  (
    LOOKAHEAD(2)
    period="."
    tok=<IDENTIFIER>
    {
      jjtThis.addNamePart(tok.image);
      jjtThis.addSpecial("period" + count, period.specialToken);
      jjtThis.addSpecial("id" + count, tok.specialToken);
      count++;
    }
  )*
}



void ClassOrInterfaceType() :
{
    Token period;
    int count = 0;
}
{
  Identifier()
  [ TypeArguments() ]
  (
    LOOKAHEAD( "." <IDENTIFIER> )
    period="."
    Identifier()
    [ TypeArguments() ]
    {
      jjtThis.addSpecial("period." + count, period.specialToken);
      count++;
    }
  )*
}


void NameList() :
{
    Token tok;
    int commaCount = 0;
}
{
  Name()
  (
    tok="," {jjtThis.addSpecial("comma." + commaCount, tok.specialToken);}
    Name()
    {commaCount++;}
  )*
}


void GenericNameList() :
{
    Token tok;
    int commaCount = 0;
}
{
  ClassOrInterfaceType()
  (
    tok="," {jjtThis.addSpecial("comma." + commaCount, tok.specialToken);}
    ClassOrInterfaceType()
    {commaCount++;}
  )*
}


/*
 * Expression syntax follows.
 */

void Expression() :
/*
 * This expansion has been written this way instead of:
 *   Assignment() | ConditionalExpression()
 * for performance reasons.
 * However, it is a weakening of the grammar for it allows the LHS of
 * assignments to be any conditional expression whereas it can only be
 * a primary expression.  Consider adding a semantic predicate to work
 * around this.
 */
{}
{
  ConditionalExpression()
  [
    AssignmentOperator() Expression()
  ]
}

void AssignmentOperator() :
{
    Token tok;
}
{
  (
    tok="="    |
    tok="*="   |
    tok="/="   |
    tok="%="   |
    tok="+="   |
    tok="-="   |
    tok="<<="  |
    tok=">>="  |
    tok=">>>=" |
    tok="&="   |
    tok="^="   |
    tok="|="
  )
  {
    jjtThis.setName(tok.image);
    jjtThis.addSpecial("operator", tok.specialToken);
  }
}

void ConditionalExpression() :
{
    Token tok;
}
{
  ConditionalOrExpression()
  [
    tok="?" {jjtThis.addSpecial("?", tok.specialToken);}
    Expression()
    tok=":" {jjtThis.addSpecial(":", tok.specialToken);}
    ConditionalExpression()
  ]
}

void ConditionalOrExpression() :
{
    Token tok;
    int count = 0;
}
{
  ConditionalAndExpression()
  (
    tok="||" {jjtThis.addSpecial("operator." + count, tok.specialToken); count++;}
    ConditionalAndExpression()
  )*
}

void ConditionalAndExpression() :
{
    Token tok;
    int count = 0;
}
{
  InclusiveOrExpression()
  (
    tok="&&" {jjtThis.addSpecial("operator." + count, tok.specialToken); count++;}
    InclusiveOrExpression()
  )*
}

void InclusiveOrExpression() :
{
    Token tok;
    int count = 0;
}
{
  ExclusiveOrExpression()
  (
    tok="|" {jjtThis.addSpecial("operator." + count, tok.specialToken); count++;}
    ExclusiveOrExpression()
  )*
}

void ExclusiveOrExpression() :
{
    Token tok;
    int count = 0;
}
{
  AndExpression()
  (
    tok="^" {jjtThis.addSpecial("operator." + count, tok.specialToken); count++;}
    AndExpression()
  )*
}

void AndExpression() :
{
    Token tok;
    int count = 0;
}
{
  EqualityExpression()
  (
    tok="&" {jjtThis.addSpecial("operator." + count, tok.specialToken); count++;}
    EqualityExpression()
  )*
}

void EqualityExpression() :
{
    Token tok;
    int count = 0;
}
{
  InstanceOfExpression()
  (
    ( tok="==" | tok="!=" )
    InstanceOfExpression()
    {
        jjtThis.addName(tok.image);
        jjtThis.addSpecial("operator." + count, tok.specialToken);
        count++;
    }
  )*
}

void InstanceOfExpression() :
{
    Token tok;
}
{
  RelationalExpression()
  [
    tok="instanceof"
    ReferenceType()
    {jjtThis.addSpecial("operator.0", tok.specialToken);}
  ]
}

void RelationalExpression() :
{
    Token tok;
    int count = 0;
}
{
  ShiftExpression()
  (
    (
      tok="<"  |
      tok=">"  |
      tok="<=" |
      tok=">="
    )
    {
      jjtThis.addName(tok.image);
      jjtThis.addSpecial("operator." + count, tok.specialToken);
      count++;
    }
    ShiftExpression()
  )*
}

void ShiftExpression() :
{
    Token tok;
    int count = 0;
}
{
  AdditiveExpression()
    (
      (
        tok="<<" |
        tok=">>" |
        tok=">>>"
      )
      {
        jjtThis.addName(tok.image);
        jjtThis.addSpecial("operator." + count, tok.specialToken);
        count++;
      }
      AdditiveExpression()
    )*
}

void AdditiveExpression() :
{
    Token tok;
    int count = 0;
}
{
  MultiplicativeExpression()
    (
      (
        tok="+" |
        tok="-"
      )
      {
        jjtThis.addName(tok.image);
        jjtThis.addSpecial("operator." + count, tok.specialToken);
        count++;
      }
      MultiplicativeExpression()
    )*
}

void MultiplicativeExpression() :
{
    Token tok;
    int count = 0;
}
{
  UnaryExpression()
    (
      (
        tok="*" |
        tok="/" |
        tok="%"
      )
      {
        jjtThis.addName(tok.image);
        jjtThis.addSpecial("operator." + count, tok.specialToken);
        count++;
      }
      UnaryExpression()
    )*
}

void UnaryExpression() :
{
    Token tok;
}
{
  (
    (
      tok="+" |
      tok="-"
    )
    UnaryExpression()
    {
      jjtThis.setName(tok.image);
      jjtThis.addSpecial("operator", tok.specialToken);
    }
  )
|
  PreIncrementExpression()
|
  PreDecrementExpression()
|
  UnaryExpressionNotPlusMinus()
}

void PreIncrementExpression() :
{
    Token tok;
}
{
  tok="++" PrimaryExpression() {jjtThis.addSpecial("operator", tok.specialToken);}
}

void PreDecrementExpression() :
{
    Token tok;
}
{
  tok="--" PrimaryExpression() {jjtThis.addSpecial("operator", tok.specialToken);}
}

void UnaryExpressionNotPlusMinus() :
{
    Token tok;
}
{
  LOOKAHEAD( {castLookahead()} )
  CastExpression()
|
  (
    (
      tok="~" |
      tok="!"
    )
    UnaryExpression()
    {
      jjtThis.setName(tok.image);
      jjtThis.addSpecial("operator", tok.specialToken);
    }
  )
|
  PostfixExpression()
}


void PostfixExpression() :
{
    Token tok = null;
}
{
  PrimaryExpression() [ tok="++" | tok="--" ]
  {
    if (tok != null) {
      jjtThis.setName(tok.image);
      jjtThis.addSpecial("operator", tok.specialToken);
    }
  }
}

void CastExpression() :
{
    Token tok;
}
{
  tok="(" {jjtThis.addSpecial("begin", tok.specialToken);}
  Type()
  tok=")" {jjtThis.addSpecial("end", tok.specialToken);}
  
  UnaryExpression()  // fix bug 758816 UnaryExpressionNotPlusMinus()
}

void PrimaryExpression() :
{}
{
  PrimaryPrefix()
  ( LOOKAHEAD( ("." "this") | ("." "super") | ("." "new") | "(" | "[" | ( "." "<") | ("." [ ReferenceTypeList() ] <IDENTIFIER>) | ReferenceTypeList() )
    PrimarySuffix()
  )*
}

void PrimaryPrefix() :
{
  Token tok;
  Token superTok;
  String name = "";
  int count = 0;
}
{
  Literal()
|
  tok="this"
  {
    jjtThis.setUsesThisModifier();
    jjtThis.setName("this");
    jjtThis.addSpecial("this", tok.specialToken);
  }
|
  LOOKAHEAD( (<IDENTIFIER> ".")* "super" "." )
  (
     tok=<IDENTIFIER> "."
     {
     	name = name + tok.image + ".";
	    jjtThis.addSpecial("this." + count, tok.specialToken);
	    count++;
     }
  )*
  superTok="super" "." tok=<IDENTIFIER>
  {
    jjtThis.setUsesSuperModifier();
    jjtThis.setCount(count);
    name = name + "super." + tok.image;
    jjtThis.setName(name);
    jjtThis.addSpecial("this", superTok.specialToken);
    jjtThis.addSpecial("id", tok.specialToken);
  }
|
  tok="(" {jjtThis.addSpecial("begin", tok.specialToken);}
  Expression()
  tok=")" {jjtThis.addSpecial("end", tok.specialToken);}
|
  AllocationExpression()
|
  LOOKAHEAD( ResultType() "." "class" )
  ResultType() "." "class"
|
  Name() //? ClassOrInterfaceType()
}

void PrimarySuffix() :
{
    Token tok;
    Token tok1;
    Token tok2;
    Token tok3;
    Token generic;
}
{
  tok="." {jjtThis.addSpecial("dot", tok.specialToken);}
  (
    tok="this"
    {
       jjtThis.setName("this");
       jjtThis.addSpecial("id", tok.specialToken);
    }
  |
    tok="super"
    {
      jjtThis.setName("super");
      jjtThis.addSpecial("id", tok.specialToken);
    }
  |
    AllocationExpression()
  |
    [ 
      ReferenceTypeList()
    ]
    tok=<IDENTIFIER>
    {
      jjtThis.setName(tok.image);
      jjtThis.addSpecial("id", tok.specialToken);
    }
  )
|
  tok="[" {jjtThis.addSpecial("[", tok.specialToken);}
  Expression()
  tok="]" {jjtThis.addSpecial("]", tok.specialToken);}
|
  Arguments() {((ASTPrimarySuffix)jjtThis).setIsArguments();}
|
  ReferenceTypeList()
}

void Literal() :
{
    Token tok = null;
}
{
  (
    tok=<INTEGER_LITERAL> |
    tok=<FLOATING_POINT_LITERAL> |
    tok=<CHARACTER_LITERAL> |
    tok=<STRING_LITERAL> |
    BooleanLiteral() |
    NullLiteral()
  )
  {
    if (tok != null) {
      jjtThis.setName(tok.image);
      jjtThis.addSpecial("id", tok.specialToken);
    }
  }
}

void BooleanLiteral() :
{
    Token tok;
}
{
  (
    tok="true" |
    tok="false"
  )
  {
    jjtThis.setName(tok.image);
    jjtThis.addSpecial("id", tok.specialToken);
  }
}

void NullLiteral() :
{
    Token tok;
}
{
  tok="null"
  {
    jjtThis.addSpecial("id", tok.specialToken);
  }
}

void Arguments() :
{
    Token tok;
}
{
  tok="(" {jjtThis.addSpecial("begin", tok.specialToken);}
  [ ArgumentList() ] [ tok=","  {jjtThis.addSpecial("comma", tok.specialToken);} ]
  tok=")" {jjtThis.addSpecial("end", tok.specialToken);}
}

void ArgumentList() :
{
    Token tok;
    int commaCount = 0;
}
{
  Expression()
  (
    LOOKAHEAD({expressionLookahead()})
    tok=","  {jjtThis.addSpecial("comma." + commaCount, tok.specialToken); commaCount++;}
    Expression()
  )*
}

void AllocationExpression() :
{
    Token tok;
}
{

  tok="new" {jjtThis.addSpecial("id", tok.specialToken);}
  (
    LOOKAHEAD(ClassOrInterfaceType())
    ClassOrInterfaceType()
    (
      ArrayDimsAndInits()
    |
      Arguments() [ ClassBody() ]
    )
  |
    PrimitiveType()
    ArrayDimsAndInits()
  )
}

/*
 * The third LOOKAHEAD specification below is to parse to PrimarySuffix
 * if there is an expression between the "[...]".
 */
void ArrayDimsAndInits() :
{
    Token tok;
    int arrayCount = 0;
}
{
  (
    // FIXME? LOOKAHEAD looks wrong??
    LOOKAHEAD(2)
    (
      LOOKAHEAD(2)
      tok="[" {jjtThis.addSpecial("[." + arrayCount, tok.specialToken);}
      Expression()
      tok="]" {jjtThis.addSpecial("]." + arrayCount, tok.specialToken);}
      {arrayCount++;}
    )+
    (
      LOOKAHEAD(4)
      tok="[" {jjtThis.addSpecial("[." + arrayCount, tok.specialToken);}
      tok="]" {jjtThis.addSpecial("]." + arrayCount, tok.specialToken);}
      {arrayCount++;}
    )*
   |
    (
      tok="[" {jjtThis.addSpecial("[." + arrayCount, tok.specialToken);}
      tok="]" {jjtThis.addSpecial("]." + arrayCount, tok.specialToken);}
      {arrayCount++;}
    )+
    ArrayInitializer()
  )
  {
    jjtThis.setArrayCount(arrayCount);
  }
}


/*
 * Statement syntax follows.
 */

void Statement() :
{
    Token tok;
}
{
  AssertionStatement()
|
  LOOKAHEAD(2)
  LabeledStatement()
|
  Block()
|
  EmptyStatement()
|
  StatementExpression() tok=";" {jjtThis.addSpecial("semicolon", tok.specialToken);}
|
  SwitchStatement()
|
  IfStatement()
|
  WhileStatement()
|
  DoStatement()
|
  ForStatement()
|
  BreakStatement()
|
  ContinueStatement()
|
  ReturnStatement()
|
  ThrowStatement()
|
  SynchronizedStatement()
|
  TryStatement()
}

void LabeledStatement() :
{
    Token tok;
    Token colon;
}
{
  tok=<IDENTIFIER> colon=":" Statement()
  {
    jjtThis.setName(tok.image);
    jjtThis.addSpecial("id", tok.specialToken);
    jjtThis.addSpecial("colon", colon.specialToken);
  }
}

void Block() :
{
    Token tok;
}
{
  tok="{" {jjtThis.addSpecial("begin", tok.specialToken);}
  ( BlockStatement() )*
  tok="}" {jjtThis.addSpecial("end", tok.specialToken);}
}

void BlockStatement() :
{
    Token tok;
    int count = 0;
}
{
  LOOKAHEAD( ( "final" | ("@" Annotation()) )* Type() <IDENTIFIER>)
  LocalVariableDeclaration()
  tok=";"
  {jjtThis.addSpecial("semicolon", tok.specialToken);}
|
  LOOKAHEAD( ( "final" | ("@" Annotation()) )* Name() "<" )
  LocalVariableDeclaration()
  tok=";"
  {jjtThis.addSpecial("semicolon", tok.specialToken);}
|
  Statement()
|
  LOOKAHEAD( ( "final" | ("@" Annotation()) )* "class")
  (
    (tok="@" Annotation() { jjtThis.addSpecial("@."+count, tok.specialToken); count++; }) |
    (tok="final" { jjtThis.addSpecial("final", tok.specialToken);  jjtThis.setFinal(true); } )
  )*
  UnmodifiedClassDeclaration()
|
  LOOKAHEAD([ "final" ] "interface")
  (
    (tok="@" Annotation() { jjtThis.addSpecial("@."+count, tok.specialToken); count++; }) |
    (tok="final" { jjtThis.addSpecial("final", tok.specialToken); jjtThis.setFinal(true); } )
  )*
  UnmodifiedInterfaceDeclaration()
}

void LocalVariableDeclaration() :
{
    Token tok = null;
    int commaCount = 0;
    int count = 0;
}
{
  ( 
    (tok="@" Annotation() { jjtThis.addSpecial("@."+count, tok.specialToken); count++; }) |
    (tok="final" { jjtThis.setUsingFinal(true); jjtThis.addSpecial("final", tok.specialToken); } )
  )*
  Type()
  VariableDeclarator()
  (
    tok="," {jjtThis.addSpecial("comma." + commaCount, tok.specialToken); commaCount++;}
    VariableDeclarator()
  )*
}

void EmptyStatement() :
{
    Token tok;
}
{
  tok=";"
  {jjtThis.addSpecial("semicolon", tok.specialToken);}
}

void StatementExpression() :
/*
 * The last expansion of this production accepts more than the legal
 * Java expansions for StatementExpression.  This expansion does not
 * use PostfixExpression for performance reasons.
 */
{
    Token tok;
}
{
  PreIncrementExpression()
|
  PreDecrementExpression()
|
  PrimaryExpression()
  [
    tok="++" {jjtThis.setName("++"); jjtThis.addSpecial("id", tok.specialToken);}
  |
    tok="--" {jjtThis.setName("--"); jjtThis.addSpecial("id", tok.specialToken);}
  |
    AssignmentOperator() Expression()
  ]
}

void SwitchStatement() :
{
    Token tok;
}
{
  tok="switch" {jjtThis.addSpecial("switch", tok.specialToken);}
  tok="(" {jjtThis.addSpecial("beginExpr", tok.specialToken);}
  Expression()
  tok=")" {jjtThis.addSpecial("endExpr", tok.specialToken);}
  tok="{" {jjtThis.addSpecial("beginBlock", tok.specialToken);}
    ( SwitchLabel() ( BlockStatement() )* )*
  tok="}" {jjtThis.addSpecial("endBlock", tok.specialToken);}
}

void SwitchLabel() :
{
    Token tok;
    Token colon;
}
{
(
  tok="case" Expression() colon=":"
|
  tok="default" colon=":"
)
  {
    jjtThis.addSpecial("id", tok.specialToken);
    jjtThis.addSpecial("colon", colon.specialToken);
  }
}

void IfStatement() :
/*
 * The disambiguating algorithm of JavaCC automatically binds dangling
 * else's to the innermost if statement.  The LOOKAHEAD specification
 * is to tell JavaCC that we know what we are doing.
 */
{
    Token tok;
}
{
  tok="if" {jjtThis.addSpecial("if", tok.specialToken);}
  tok="(" {jjtThis.addSpecial("beginExpr", tok.specialToken);}
  Expression()
  tok=")" {jjtThis.addSpecial("endExpr", tok.specialToken);}
  Statement()
  [
    LOOKAHEAD(1)
    tok="else" {jjtThis.setHasElse(); jjtThis.addSpecial("else", tok.specialToken);}
    Statement()
  ]
}

void WhileStatement() :
{
    Token tok;
}
{
  tok="while" {jjtThis.addSpecial("while", tok.specialToken);}
  tok="(" {jjtThis.addSpecial("beginExpr", tok.specialToken);}
  Expression()
  tok=")" {jjtThis.addSpecial("endExpr", tok.specialToken);}
  Statement()
}


void ForStatement() :
{
    Token tok;
}
{
  tok="for" {jjtThis.addSpecial("for", tok.specialToken);}
  tok="(" {jjtThis.addSpecial("beginExpr", tok.specialToken);}
  (
      
    LOOKAHEAD( {forLookahead()} )
    ( 
      LocalVariableDeclaration()
      tok=":" {jjtThis.addSpecial("loopover", tok.specialToken);}
      Expression()
    )
    |
    (
      [ ForInit() ]
      tok=";" {jjtThis.addSpecial("init", tok.specialToken);}
      [ Expression() ]
      tok=";" {jjtThis.addSpecial("test", tok.specialToken);}
      [ ForUpdate() ]
    )
  )

  tok=")" {jjtThis.addSpecial("endExpr", tok.specialToken);}
  Statement()
}

void ForInit() :
{}
{
  LOOKAHEAD( ( "final" | ("@" Annotation()) )* ( Type() <IDENTIFIER> | Name() "<" ) )
  (
    LocalVariableDeclaration()
  )
|
  (
    StatementExpressionList()
  )
}



void DoStatement() :
{
    Token tok;
}
{
  tok="do" {jjtThis.addSpecial("do", tok.specialToken);}
  Statement()
  tok="while" {jjtThis.addSpecial("while", tok.specialToken);}
  tok="(" {jjtThis.addSpecial("beginExpr", tok.specialToken);}
  Expression()
  tok=")" {jjtThis.addSpecial("endExpr", tok.specialToken);}
  tok=";" {jjtThis.addSpecial("semicolon", tok.specialToken);}
}



void StatementExpressionList() :
{
    Token tok;
    int commaCount = 0;
}
{
  StatementExpression()
  (
    tok="," {jjtThis.addSpecial("comma." + commaCount, tok.specialToken);  commaCount++;}
    StatementExpression()
  )*
}

void ForUpdate() :
{}
{
  StatementExpressionList()
}

void BreakStatement() :
{
    Token tok;
}
{
  tok="break" {jjtThis.addSpecial("break", tok.specialToken);}
  [ tok=<IDENTIFIER> {jjtThis.setName(tok.image); jjtThis.addSpecial("id", tok.specialToken);}]
  tok=";" {jjtThis.addSpecial("semicolon", tok.specialToken);}

}

void ContinueStatement() :
{
    Token tok;
}
{
  tok="continue" {jjtThis.addSpecial("continue", tok.specialToken);}
  [ tok=<IDENTIFIER> {jjtThis.setName(tok.image); jjtThis.addSpecial("id", tok.specialToken);}]
  tok=";" {jjtThis.addSpecial("semicolon", tok.specialToken);}
}

void ReturnStatement() :
{
    Token tok;
}
{
  tok="return" {jjtThis.addSpecial("return", tok.specialToken);}
  [ Expression() ]
  tok=";" {jjtThis.addSpecial("semicolon", tok.specialToken);}
}

void ThrowStatement() :
{
    Token tok;
}
{
  tok="throw" {jjtThis.addSpecial("throw", tok.specialToken);}
  Expression()
  tok=";" {jjtThis.addSpecial("semicolon", tok.specialToken);}
}

void SynchronizedStatement() :
{
    Token tok;
}
{
  tok="synchronized"  {jjtThis.addSpecial("synchronized", tok.specialToken);}
  tok="("  {jjtThis.addSpecial("beginExpr", tok.specialToken);}
  Expression()
  tok=")"  {jjtThis.addSpecial("endExpr", tok.specialToken);}
  Block()
}

void TryStatement() :
/*
 * Semantic check required here to make sure that at least one
 * finally/catch is present.
 */
{
    Token tok;
    int catchCount = 0;
}
{
  tok="try"  {jjtThis.addSpecial("try", tok.specialToken);}
  Block()
  (
    tok="catch"  {jjtThis.setHasCatch(); jjtThis.addSpecial("catch" + catchCount, tok.specialToken);}
    tok="("  {jjtThis.addSpecial("beginExpr" + catchCount, tok.specialToken);}
    FormalParameter()
    tok=")"  {jjtThis.addSpecial("endExpr" + catchCount, tok.specialToken);}
    Block()
    {catchCount++;}
  )*
  [
    tok="finally"  {jjtThis.setHasFinally(); jjtThis.addSpecial("finally", tok.specialToken);}
    Block()
  ]
}

void AssertionStatement() :
{
    Token tok;
}
{
  tok = "assert" {
    if (!jdk1_4) {
      System.err.println("WARNING: assert declared in pre-jdk 1.4 target");
    }
    jjtThis.addSpecial("assert", tok.specialToken);
  }
  Expression()
  [
	  tok=":"  {jjtThis.addSpecial("colon", tok.specialToken);}
	  Expression()
  ]
  tok=";"
  {jjtThis.addSpecial("semicolon", tok.specialToken);
  }
}


